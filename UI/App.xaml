             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:SFMControls="clr-namespace:SFMControls;assembly=SFMControls"
             StartupUri="Login.xaml" DispatcherUnhandledException="Application_DispatcherUnhandledException">
    <Application.Resources>
        <ResourceDictionary Source="Skins/EmptySkin.xaml">
        	<ResourceDictionary.MergedDictionaries>
        		<ResourceDictionary Source="Skins/EmptySkin.xaml"/>
