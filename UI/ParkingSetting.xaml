                            <SFMControls:ButtonSfm Name="btnDelCamera" Content="删  除" HorizontalAlignment="Left" Height="auto" Margin="681,151,0,0" VerticalAlignment="Top" Width="100" Click="btnDelCamera_Click" FormName="网络摄像机设置" ItemName="BtnDelete" Background="{x:Null}"/>
                            <SFMControls:ButtonSfm Name="btnSaveCamera" Content="确  定" HorizontalAlignment="Left" Height="auto" Margin="681,229,0,0" VerticalAlignment="Top" Width="100" Click="btnSaveCamera_Click" FormName="网络摄像机设置" ItemName="BtnUpdate" Background="{x:Null}">

                            </SFMControls:ButtonSfm>
                        </Grid>
                    </GroupBox>
                    <Border BorderBrush="Black" BorderThickness="1" HorizontalAlignment="Left" Height="174" Margin="37,103,0,0" VerticalAlignment="Top" Width="811">
                        <Grid>
                            <ComboBox x:Name="cboImageAutoDelTime" HorizontalAlignment="Left" Height="auto" Margin="80,98,0,0" VerticalAlignment="Top" Width="51" IsEditable="True" KeyDown="ComboBox_KeyDown">
                                <ComboBoxItem Content="0"/>
                                <ComboBoxItem IsSelected="True" Content="1"/>
                                <ComboBoxItem Content="2"/>
                                <ComboBoxItem Content="3"/>
                                <ComboBoxItem Content="4"/>
                                <ComboBoxItem Content="5"/>
                                <ComboBoxItem Content="6"/>
                                <ComboBoxItem Content="7"/>
                                <ComboBoxItem Content="8"/>
                                <ComboBoxItem Content="9"/>
                                <ComboBoxItem Content="10"/>
                                <ComboBoxItem Content="11"/>
                                <ComboBoxItem Content="12"/>
                                <ComboBoxItem Content="13"/>
                                <ComboBoxItem Content="14"/>
                                <ComboBoxItem Content="15"/>
                                <ComboBoxItem Content="16"/>
                                <ComboBoxItem Content="17"/>
                                <ComboBoxItem Content="18"/>
                                <ComboBoxItem Content="19"/>
                                <ComboBoxItem Content="20"/>
                                <ComboBoxItem Content="21"/>
                                <ComboBoxItem Content="22"/>
                                <ComboBoxItem Content="23"/>
                            </ComboBox>
                            <Label Content="图像保存路径" HorizontalAlignment="Left" Height="auto" Margin="10,31,0,0" VerticalAlignment="Top" Width="auto"/>
                            <SFMControls:ButtonSfm x:Name="btnImgSavePath" Content="浏览" HorizontalAlignment="Left" Height="auto" Margin="497,31,0,0" VerticalAlignment="Top" Width="101" Click="btnImgSavePath_Click" Background="{x:Null}"/>
                            <Label Content="天以前的图片" HorizontalAlignment="Left" Height="auto" Margin="352,95,0,0" VerticalAlignment="Top" Width="auto"/>
                            <ComboBox x:Name="cboImageSaveDays" HorizontalAlignment="Left" Height="auto" Margin="273,95,0,0" VerticalAlignment="Top" Width="63" IsEditable="True" KeyDown="cboImageSaveDays_KeyDown">
                                <ComboBoxItem IsSelected="True" Content="30"/>
                                <ComboBoxItem Content="60"/>
                                <ComboBoxItem Content="90"/>
                                <ComboBoxItem Content="120"/>
                            </ComboBox>
                            <CheckBox x:Name="chkImageAutoDel" Content="每天" HorizontalAlignment="Left" Height="auto" Margin="10,101,0,0" VerticalAlignment="Top" Width="auto"/>
                            <TextBox x:Name="txtImgSavePath" HorizontalAlignment="Left" Height="auto" Margin="145,31,0,0" Text="E:\" VerticalAlignment="Top" Width="337" TextWrapping="NoWrap"  HorizontalScrollBarVisibility="Auto"/>
                            <Label Content="点自动删除" HorizontalAlignment="Left" Height="auto" Margin="145,96,0,0" VerticalAlignment="Top" Width="auto"/>

                        </Grid>
                    </Border>
                    <SFMControls:ButtonSfm x:Name="cmdSelectNetCameraSet" Content="网络摄像机设置" HorizontalAlignment="Left" Height="auto" Margin="217,17,0,0" VerticalAlignment="Top" Width="178" FormName="网络摄像机设置" ItemName="CmdView" Click="cmdSelectNetCameraSet_Click" Background="{x:Null}"/>
                    <CheckBox Name="chkJF" Content="启用计费型摄像机" HorizontalAlignment="Left" Height="auto" Margin="461,20,0,0" VerticalAlignment="Top" Width="auto"/>
                </Grid>
            </TabItem>

