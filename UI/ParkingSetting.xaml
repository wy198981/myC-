                                    <DataGridTextColumn Header="车类"  Binding="{Binding CardType,Mode=TwoWay}" Width="100" MaxWidth="100"/>
                                    <DataGridTextColumn Header="月数"  Binding="{Binding FreeMinute,Mode=TwoWay}" Width="50" MaxWidth="50"/>
                                    <DataGridTextColumn Header="金额(元)"  Binding="{Binding JE,Mode=TwoWay}" Width="200" MaxWidth="200"/>
                                </DataGrid.Columns>
                            </DataGrid>
                            <ComboBox Name="cboMonthRuleType" HorizontalAlignment="Left" Height="30" Margin="379,10,0,0" VerticalAlignment="Top" Width="105" Background="{x:Null}"/>
