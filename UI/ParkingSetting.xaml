<SFMControls:WindowBase
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
        xmlns:code="clr-namespace:UI"
        xmlns:SFMControls="clr-namespace:SFMControls;assembly=SFMControls"
        xmlns:src="clr-namespace:SFMControls;assembly=SFMControls"
        xmlns:core="clr-namespace:System;assembly=mscorlib" 
        xmlns:cvt="clr-namespace:UI"
        x:Class="UI.ParkingSetting"
        Style="{DynamicResource ResourceKey=WindowStyle}" Title="车场设置" Height="753.015" Width="1361.195" WindowStartupLocation="CenterScreen" Loaded="Window_Loaded" Closing="Window_Closing" ResizeMode="NoResize">
    <Window.Resources>
        <cvt:ItosConverter x:Key="cvtDate"/>
        <cvt:IOConverter x:Key="cvtIO"/>
        <cvt:JHConverter x:Key="cvtJH"/>
        <cvt:BSConverter x:Key="cvtBS"/>
        <cvt:XYConverter x:Key="cvtXY"/>
        <cvt:CIPConverter x:Key="cvtCIP"/>
        <cvt:VideoTypeConverter x:Key="cvtVideoType"/>
        <cvt:PersonVideoConverter x:Key="cvtPersonVideo"/>
    </Window.Resources>


    <Grid Name="gd1">
        <TabControl Name="tbPark" TabStripPlacement="Left" Margin="0,0,0,56" Background="{x:Null}">
			<TabControl.Resources>
				<Style TargetType="TabItem" BasedOn="{StaticResource TabItemStyleBase}">
					<Setter Property="Width" Value="200"/>
					<Setter Property="Height" Value="40"/>
				</Style>
			</TabControl.Resources>
            <TabItem x:Name="tbCheDaoSet" Header="车道设置">
                <Grid>
                    <StackPanel Name="spCheDao"  Height="43" VerticalAlignment="Top" Width="931" HorizontalAlignment="Left" Orientation="Horizontal">
                        <SFMControls:ButtonSfm Name="btnAddCheDao" Content="添加车道" Height="43" VerticalAlignment="Top" Width="110" Click="btnAddCheDao_Click" ItemName="BtnAdd" FormName="车道设置" />
                        <SFMControls:ButtonSfm Name="btnDeleteCheDao" Content="删除车道" Height="43" VerticalAlignment="Top" Width="110" Click="btnDeleteCheDao_Click" ItemName="BtnDelete" FormName="车道设置" Background="{x:Null}" />
                    </StackPanel>
                    <DataGrid x:Name="dgLane"  Margin="0,43,0,0"  ItemsSource="{Binding}"  AutoGenerateColumns="False" CanUserAddRows="False" BeginningEdit="dgLane_BeginningEdit" CellEditEnding="dgLane_CellEditEnding" HorizontalAlignment="Left"  Background="{x:Null}">
                        <DataGrid.Columns>
                            <DataGridComboBoxColumn Header="车道类型" ItemsSource="{x:Static cvt:BinModel.lstInOut}" TextBinding="{Binding InOut, Converter={StaticResource cvtIO}, Mode=TwoWay}"/>
                            <DataGridTextColumn x:Name="dgtxtLane" Header="车道名称" Binding="{Binding InOutName, Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" IsReadOnly="True"/>
                            <DataGridComboBoxColumn Header="控制机号" ItemsSource="{x:Static cvt:BinModel.lstCtrlNumber}" SelectedItemBinding="{Binding CtrlNumber,Mode=TwoWay}"/>
                            <DataGridComboBoxColumn Header="开闸机号" Visibility="Hidden" IsReadOnly="True"/>
                            <DataGridComboBoxColumn Header="开闸方式" ItemsSource="{x:Static cvt:BinModel.lstOpenType}" TextBinding="{Binding OpenType, Converter={StaticResource cvtDate}, Mode=TwoWay}"/>
                            <DataGridComboBoxColumn Header="车像抓拍" x:Name="dcboCameraIP" SelectedValueBinding="{Binding CameraIP}"/>
                            <DataGridComboBoxColumn x:Name="dcboPersonID" Header="人像抓拍" Visibility="Hidden" SelectedValueBinding="{Binding PersonVideo}"/>
                            <DataGridComboBoxColumn Header="大小车场" ItemsSource="{x:Static cvt:BinModel.lstBigSmall}" TextBinding="{Binding BigSmall, Converter={StaticResource cvtBS}, Mode=TwoWay}"/>
                            <DataGridCheckBoxColumn Header="是否在线" Binding="{Binding OnLine, Mode=TwoWay}"/>
                            <DataGridCheckBoxColumn Header="检测口" Binding="{Binding CheckPortID, Mode=TwoWay}"/>
                            <DataGridCheckBoxColumn Header="出卡机" Binding="{Binding HasOutCard, Mode=TwoWay}"/>
                            <DataGridComboBoxColumn Header="通讯协议" ItemsSource="{x:Static cvt:BinModel.lstXieYi}" TextBinding="{Binding XieYi, Converter={StaticResource cvtXY}, Mode=TwoWay}"/>
                            <DataGridTextColumn Header="IP地址" Binding="{Binding IP,Mode=TwoWay}"/>
                            <DataGridTextColumn Header="ID" Binding="{Binding ID,Mode=TwoWay}" Visibility="Hidden" IsReadOnly="True"/>
                        </DataGrid.Columns>
                    </DataGrid>
                </Grid>
            </TabItem>

            <TabItem Header="视频参数设置" Margin="-2,-2,-1,-2" Height="40" FontSize="16">
                <Grid>
                    <CheckBox Name="chkEnableNetVideo" Content="启用网络视频" HorizontalAlignment="Left" Height="auto" Margin="38,20,0,0" VerticalAlignment="Top" Width="auto" Click="chkEnableNetVideo_Click"/>
                    <CheckBox Name="chkVideo4" Content="启用四路视频" HorizontalAlignment="Left" Height="auto" Margin="38,58,0,0" VerticalAlignment="Top" Width="auto"/>
                    <CheckBox Name="chkPersonVideo" Content="启用副摄像机抓拍" HorizontalAlignment="Left" Height="auto" Margin="217,58,0,0" VerticalAlignment="Top" Width="auto"  Click="chkPersonVideo_Click" Background="{x:Null}"/>
                    <CheckBox Name="chkZJZP" Content="证件抓拍" HorizontalAlignment="Left" Height="auto" Margin="461,58,0,0" VerticalAlignment="Top" Width="auto"/>
