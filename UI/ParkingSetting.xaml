                                <ComboBoxItem IsSelected="True">临时车</ComboBoxItem>
                                <ComboBoxItem>储值车</ComboBoxItem>
                                <ComboBoxItem>临时车和储值车</ComboBoxItem>
                            </ComboBox>
                            <ComboBox Name="cmbStopTimeType" HorizontalAlignment="Left" Height="Auto" Margin="224,10,0,0" VerticalAlignment="Top" Width="136" Background="{x:Null}">
                                <ComboBoxItem>0到24点</ComboBoxItem>
                                <ComboBoxItem IsSelected="true">停车24小时</ComboBoxItem>
                            </ComboBox>

                        </Grid>
                    </GroupBox>
                    <CheckBox Name="chkDiscount" Content="收费可打折" HorizontalAlignment="Left" Height="auto" Margin="41,199,0,0" VerticalAlignment="Top" Width="125"/>
