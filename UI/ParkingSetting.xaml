                    <SFMControls:ButtonSfm Name="btnSelectMonthRule" Content="月租车规则自定义" HorizontalAlignment="Left" Height="auto" Margin="692,239,0,0" VerticalAlignment="Top" Width="174" Click="btnSelectMonthRule_Click" FormName="月租车规则自定义" ItemName="CmdView" Background="{x:Null}"/>
