                                        <DataGridTemplateColumn.CellTemplate>
                                            <DataTemplate>
                                                <ComboBox Name="cboVideoType" Loaded="cboVideoType_Loaded"  SelectedItem="{Binding VideoType,Mode=TwoWay}" SelectionChanged="cboVideoType_SelectionChanged"></ComboBox>
                                            </DataTemplate>
                                        </DataGridTemplateColumn.CellTemplate>
                                    </DataGridTemplateColumn>-->

                                    <DataGridComboBoxColumn x:Name="dcboVideoType" Header="设备类型" Width="auto" SelectedValueBinding="{Binding VideoType}"/>
                                    <DataGridTextColumn Header="相机IP" Binding="{Binding VideoIP}"  Width="2*"/>
                                    <DataGridTextColumn Header="端口号" Binding="{Binding VideoPort}" Width="*"/>
                                    <DataGridTextColumn Header="用户名" Binding="{Binding VideoUserName}" Width="*"/>
                                    <DataGridTextColumn Header="密码" Binding="{Binding VideoPassWord}" Width="*"/>
                                </DataGrid.Columns>
                            </DataGrid>
