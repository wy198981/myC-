            <ComboBoxItem>甘</ComboBoxItem>
            <ComboBoxItem>青</ComboBoxItem>
            <ComboBoxItem>宁</ComboBoxItem>
            <ComboBoxItem>新</ComboBoxItem>
        </ComboBox>
        <TextBox Name="txtCPH" HorizontalAlignment="Left" Height="31" Margin="140,55,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="86" FontSize="15" TextChanged="txtCPH_TextChanged" KeyDown="txtCPH_KeyDown"/>
        <Label Content="车道名称:" HorizontalAlignment="Left" Height="auto" Margin="242,55,0,0" VerticalAlignment="Top" Width="auto" FontSize="15"/>
        <ComboBox Name="cmbGateName" HorizontalAlignment="Left" Height="30" Margin="321,55,0,0" VerticalAlignment="Top" Width="137" IsEditable="True" FontSize="15"/>
        <SFMControls:ButtonSfm Name="btnCancel" Content="取   消" HorizontalAlignment="Left" Height="28" Margin="472,57,0,0" VerticalAlignment="Top" Width="80" FontSize="15" Click="btnCancel_Click"/>
        <WindowsFormsHost Canvas.Left="438" Canvas.Top="59" Margin="10,101,10,10">
            <wf:PictureBox x:Name="picInPic"  Dock="Fill" BackColor="Black" SizeMode="StretchImage">
            </wf:PictureBox>
        </WindowsFormsHost>
    </Grid>
</SFMControls:WindowBase>
