                        <Grid.RowDefinitions>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="15*"/>
                        </Grid.RowDefinitions>

                        <StackPanel Name="spCheDao" Orientation="Horizontal" Grid.Row="0">

                            <SFMControls:ButtonSfm Name="btnAddCheDao" Content="添加车道"  Width="110" ItemName="BtnAdd" FormName="车道设置" Click="btnAddCheDao_Click" Margin="10,0,0,0"/>
                            <SFMControls:ButtonSfm Name="btnDeleteCheDao" Content="删除车道" Width="110" ItemName="BtnDelete" FormName="车道设置" Click="btnDeleteCheDao_Click" Margin="10,0,0,0"/>
                        </StackPanel>

                        <DataGrid x:Name="dgLane" Grid.Row="1" ItemsSource="{Binding}"  AutoGenerateColumns="False" CanUserAddRows="False" HorizontalAlignment="Left"  BeginningEdit="dgLane_BeginningEdit" CellEditEnding="dgLane_CellEditEnding" BorderThickness="0" Background="{x:Null}">
                            <DataGrid.Columns>
                                <DataGridComboBoxColumn Header="车道类型" ItemsSource="{x:Static cvt:BinModel.lstInOut}" TextBinding="{Binding InOut, Converter={StaticResource cvtIO}, Mode=TwoWay}"/>
                                <DataGridTextColumn x:Name="dgtxtLane" Header="车道名称" Binding="{Binding InOutName, Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"/>
                                <DataGridComboBoxColumn Header="控制机号" ItemsSource="{x:Static cvt:BinModel.lstCtrlNumber}" SelectedItemBinding="{Binding CtrlNumber,Mode=TwoWay}"/>
                                <DataGridComboBoxColumn Header="开闸机号" Visibility="Hidden" IsReadOnly="True"/>
                                <DataGridComboBoxColumn Header="开闸方式" ItemsSource="{x:Static cvt:BinModel.lstOpenType}" TextBinding="{Binding OpenType, Converter={StaticResource cvtDate}, Mode=TwoWay}" Visibility="Collapsed"/>
                                <DataGridComboBoxColumn Header="车像抓拍" x:Name="dcboCameraIP" ItemsSource="{x:Static cvt:BinModel.lstCameraIP}" TextBinding="{Binding CameraIP, Converter={StaticResource cvtCIP}, Mode=TwoWay}"/>
                                <DataGridComboBoxColumn x:Name="dcboPersonID" Header="副相机抓拍" Visibility="Hidden" SelectedValueBinding="{Binding PersonVideo}"/>
                                <DataGridComboBoxColumn Header="大小车场" ItemsSource="{x:Static cvt:BinModel.lstBigSmall}" TextBinding="{Binding BigSmall, Converter={StaticResource cvtBS}, Mode=TwoWay}"/>
                                <DataGridCheckBoxColumn Header="是否在线" Binding="{Binding OnLine, Mode=TwoWay}"/>
                                <DataGridCheckBoxColumn Header="备用1" Binding="{Binding CheckPortID, Mode=TwoWay}"/>
                                <DataGridCheckBoxColumn Header="备用2" Binding="{Binding HasOutCard, Mode=TwoWay}"/>
                                <DataGridComboBoxColumn Header="通讯协议" ItemsSource="{x:Static cvt:BinModel.lstXieYi}" TextBinding="{Binding XieYi, Converter={StaticResource cvtXY}, Mode=TwoWay}"/>
                                <DataGridTextColumn Header="控制机IP" Binding="{Binding IP,Mode=TwoWay}"/>
                                <DataGridTextColumn Header="ID" Binding="{Binding ID,Mode=TwoWay}" Visibility="Hidden" IsReadOnly="True"/>
                            </DataGrid.Columns>
                        </DataGrid>
                    </Grid>
                </TabItem>


