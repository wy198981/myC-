                                                </DataTemplate>
                                            </DataGridTemplateColumn.CellTemplate>
                                            <DataGridTemplateColumn.CellEditingTemplate>
                                                <DataTemplate>
                                                    <ComboBox x:Name="cboValue" ItemsSource="{x:Static cvt:BinModel.lstVideoType}" Width="200" SelectedItem="{Binding VideoType,Mode=TwoWay}" Text="{Binding VideoType}"/>
                                                </DataTemplate>
                                            </DataGridTemplateColumn.CellEditingTemplate>
                                        </DataGridTemplateColumn>


                                        <DataGridTextColumn Header="相机IP" Binding="{Binding VideoIP}"  Width="2*"/>
                                        <DataGridTextColumn Header="端口号" Binding="{Binding VideoPort}" Width="*"/>
                                        <DataGridTextColumn Header="用户名" Binding="{Binding VideoUserName}" Width="*"/>
                                        <DataGridTextColumn Header="密码" Binding="{Binding VideoPassWord}" Width="*"/>
                                    </DataGrid.Columns>
                                </DataGrid>

                                <SFMControls:ButtonSfm Name="btnAddCamera" Content="添  加" HorizontalAlignment="Left" Margin="712,45,0,0" VerticalAlignment="Top" Width="100" Click="btnAddCamera_Click" FormName="网络摄像机设置" ItemName="BtnAdd"/>
