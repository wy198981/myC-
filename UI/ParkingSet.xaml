                                <CheckBox x:Name="chkPersonVideo" Content="启用副摄像机抓拍" HorizontalAlignment="Left" Height="auto" Margin="193,58,0,0" VerticalAlignment="Top" Width="auto"  Click="chkPersonVideo_Click" Background="{x:Null}"/>
                                <CheckBox x:Name="chkZJZP" Content="证件抓拍" HorizontalAlignment="Left" Height="auto" Margin="407,58,0,0" VerticalAlignment="Top" Width="auto"/>
                                <src:ButtonSfm x:Name="cmdSelectNetCameraSet" Content="网络摄像机设置" HorizontalAlignment="Left" Margin="193,12.05,0,0" VerticalAlignment="Top" Width="152" FormName="网络摄像机设置" ItemName="CmdView" Click="cmdSelectNetCameraSet_Click"/>

