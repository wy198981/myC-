                                        <!--<DataGridComboBoxColumn x:Name="dcboVideoType" Header="设备类型" Width="auto">
                                        <DataGridComboBoxColumn.ElementStyle>
                                            <Style TargetType="ComboBox">
                                                <Setter Property="ItemsSource" Value="{Binding Path=BinModel.lstVideoType,ElementName=this}"/>
                                                <Setter Property="SelectedValue" Value="{Binding Path=VideoType}"/>
                                                <Setter Property="DisplayMemberPath" Value="{Binding Path=VideoType}"></Setter>
                                                <Setter Property="IsEditable" Value="True"></Setter>
                                            </Style>
                                        </DataGridComboBoxColumn.ElementStyle>
                                    </DataGridComboBoxColumn>-->

