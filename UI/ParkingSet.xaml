                                <src:ButtonSfm x:Name="btnImgSavePath" Content="浏览" HorizontalAlignment="Left" Margin="504,9.05,0,0" VerticalAlignment="Top" Width="101" Click="btnImgSavePath_Click"/>
                                <CheckBox x:Name="chkImageAutoDel" Content="每天" HorizontalAlignment="Left" Height="auto" Margin="38,66,0,0" VerticalAlignment="Top" Width="auto"/>
