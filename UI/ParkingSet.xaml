        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:SFMControls="clr-namespace:SFMControls;assembly=SFMControls"
        xmlns:src="clr-namespace:SFMControls;assembly=SFMControls"
        xmlns:wf ="clr-namespace:System.Windows.Forms;assembly=System.Windows.Forms"
        xmlns:core="clr-namespace:System;assembly=mscorlib" 
        xmlns:cvt="clr-namespace:UI"
        Title="车场设置" Style="{DynamicResource WindowStyle}" Height="820" Width="1350" FontSize="14" FontFamily="Arial,SimSun" WindowStartupLocation="CenterScreen" ResizeMode="NoResize" Loaded="Window_Loaded" Closing="Window_Closing" BorderBrush="#FFA6A6A6"  Icon="Resources/Park.ico">
