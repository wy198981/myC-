                            </Grid>
                        </GroupBox>

                        <GroupBox Name="grpCamera" Header="网络摄像机设置" HorizontalAlignment="Left" Height="288" Margin="10,323,0,0" VerticalAlignment="Top" Width="834">
                            <Grid Name="gdNetCamera">

                                <DataGrid Name="dgvVideo" HorizontalAlignment="Left" VerticalAlignment="Top"  Width="680" ItemsSource="{Binding}" CanUserAddRows="False" BeginningEdit="dgvVideo_BeginningEdit" CellEditEnding="dgvVideo_CellEditEnding" AutoGenerateColumns="False" Margin="18,14,0,0">
                                    <DataGrid.Columns>

