                                            <src:ButtonSfm Name="btnCloseDev" Content="关闭" HorizontalAlignment="Left" Margin="173,94,0,0" VerticalAlignment="Top" Width="71" Click="btnCloseDev_Click"/>
                                            <src:ButtonSfm Name="btnUpdateIP" Content="修改IP" HorizontalAlignment="Left" Margin="319,94,0,0" VerticalAlignment="Top" Width="77" Click="btnUpdateIP_Click"/>
                                            <PasswordBox Name="txtLoginPwd" HorizontalAlignment="Left" Height="auto" Margin="296,55,0,0" VerticalAlignment="Top" Width="133" Password="admin"/>
