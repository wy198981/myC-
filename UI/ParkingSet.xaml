                                            <TextBox Name="txtLoginPort" HorizontalAlignment="Left" Height="auto" Margin="296,13,0,0" TextWrapping="Wrap" Text="80" VerticalAlignment="Top" Width="133"/>

                                            <TextBox Name="txtLoginUserName" HorizontalAlignment="Left" Height="auto" Margin="81,57,0,0" TextWrapping="Wrap" Text="admin" VerticalAlignment="Top" Width="145"/>

